{
	"name": "devcontainer",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"runArgs": [
		"--name",
		"devcontainer"
	],
	"remoteUser": "root",
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.Go",
				"rust-lang.rust-analyzer",
				"ms-vscode.vscode-typescript-next",
				"ms-vscode.cpptools",
				"ms-vscode.cmake-tools"
			]
		}
	}
}